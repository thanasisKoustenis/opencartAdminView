{{header}}
{{column_left}}

<div id='content'>
	<div class='page-header'>
		<div class='container-fluid'>
			<div class='pull-right'>
				<button class='btn btn-primary'>Save</button>
				<a class='btn btn-danger'>Cancel</a>
			</div>
			<h1>{{title}}</h1>
			<ul class='breadcrumb'>
				{% for breadcrumb in breadcrumbs %}
					<li>
						<a href='{{breadcrumb.href}}'>
							{{breadcrumb.text}}
						</a>
					</li>
				{% endfor %}
			</ul>
		</div>
	</div>
    <div class='container-fluid'>
        
    </div>
</div>

{{footer}}
